{
  "family": "poll-position-ui-task",
  "containerDefinitions": [
    {
      "name": "poll-position-ui",
      "image": "<AWS_ACCOUNT_ID>.dkr.ecr.<AWS_REGION>.amazonaws.com/poll-position-ui:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "environment": [
        {
          "name": "S3_BUCKET",
          "value": "https://your-s3-bucket-name.s3.amazonaws.com"
        }
      ]
    }
  ]
}